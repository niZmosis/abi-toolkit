{
  "private": true,
  "name": "ethereum-abi-types-generator",
  "version": "2.0.0",
  "description": "Generate types from an ethereum ABI json file.",
  "keywords": [
    "ethereum",
    "blockchain",
    "types",
    "abi",
    "typescript"
  ],
  "homepage": "https://github.com/joshstevens19/ethereum-abi-types-generator#readme",
  "author": "joshstevens19@hotmail.co.uk",
  "license": "MIT",
  "scripts": {
    "clean": "lerna clean --yes",
    "lint": "pnpm eslint --ext .ts,.tsx,.js,.jsx . --fix",
    "prettier": "prettier --write .",
    "docs": "typedoc",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "build": "lerna run build",
    "start": "lerna run start --parallel --ignore 'examples'",
    "rebuild": "pnpm clean && pnpm build",
    "prepack": "pnpm build",
    "publish": "lerna run prepublish && lerna publish from-package --yes",
    "publish:dry": "lerna run prepublish && lerna publish from-package --yes --dry-run"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.0",
    "@vitest/coverage-v8": "^2.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^4.0.0",
    "lerna": "^8.1.6",
    "prettier": "^3.3.2",
    "typedoc": "^0.26.4",
    "typedoc-github-wiki-theme": "^2.0.0",
    "typedoc-plugin-markdown": "^4.1.2",
    "typescript": "^5.5.3",
    "vitest": "^2.0.2"
  }
}
